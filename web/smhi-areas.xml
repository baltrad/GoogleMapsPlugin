<?xml version='1.0' encoding='iso-8859-1'?>
<!-- 
  Note, you must have a valid google map projection in order to
  get reliable behaviour.
  Note, order is cruicial and will be shown in the order they are
  listed here.
  
  product:
    name = name of area, data catalog must exist
    description = what should be shown as option text
    zoom = default zoom level 1 - 9
-->

<productlist>
  <product name="swegmaps_2000" description="Swedish Composite" zoom="4"/>
  <product name="fingmaps_2000" description="Finnish Composite" zoom="4"/>
  <product name="polgmaps_2000" description="Polish Composite" zoom="4"/>
  <product name="bltgmaps_4000" description="BALTRAD 2 km" zoom="3"/>
</productlist>


